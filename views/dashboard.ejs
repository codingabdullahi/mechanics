<script>
    document.addEventListener("DOMContentLoaded", function () {
    const mobileElement = document.querySelector(".mobile");
    const undernavElement = document.querySelector(".undernav");

    if (mobileElement && undernavElement) {
        mobileElement.addEventListener("click", function () {
            undernavElement.classList.toggle("block");
        });
    } else {
        console.error("Elements not found.");
    }
    console.log('hulo');
});
